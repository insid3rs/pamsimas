        <record model="ir.ui.view" id="firm_form_view">
            <field name="name">firm.form</field>
            <field name="model">pamsimas.firm</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form string="firm Form">
                    <field name="name" />
                    <notebook colspan="4">
                        <page string="Description">
                            <field name="description" nolabel="1" colspan="4" />
                        </page>
                    </notebook>
                </form>
            </field>
        </record>

